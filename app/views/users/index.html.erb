<%# home page %>
<h1 align="center">Todos los usuarios</h1>
<div align="center">
 <% @users.each do |user| %>
  <ul class="listing">
   <div class="row">
    <div class="well col-md-4 col-md-offset-4">
     <li><%=image_tag user.photo, size: 150 %></li>
     <li class="article-title"><%= link_to user.username, user_path(user) %></li>
    </div>
   </div>
  </ul>
 <% end %>
</div>